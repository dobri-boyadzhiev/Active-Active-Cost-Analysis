# Flask Configuration
# Copy this file to .env and update with your values

# Secret key for Flask sessions (generate with: python -c "import os; print(os.urandom(24).hex())")
SECRET_KEY=your-secret-key-here

# Debug mode (set to False in production)
FLASK_DEBUG=False

# Host (127.0.0.1 for localhost only, 0.0.0.0 for all interfaces)
FLASK_HOST=127.0.0.1

# Port
FLASK_PORT=5000

# Database path (optional, defaults to ../aa_report_cache.db)
# DB_PATH=../aa_report_cache.db

